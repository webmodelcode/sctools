{
  "entryPoints": [
    "./src/main.tsx",
    "./src/content.tsx",
    "./src/**/*.tsx",
    "./src/**/*.ts"
  ],
  "exclude": [
    "**/*.test.tsx",
    "./src/components/ui/**",
    "./src/vite-env.d.ts",
    "./src/test/*",
    "./src/lib/*"
  ],
  "out": "docs",
  "name": "ScTools Chrome Extension",
  "includeVersion": true,
  "readme": "README.md",
  "categorizeByGroup": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true
}
